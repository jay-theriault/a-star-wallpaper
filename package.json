    "*.{json,html,css,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^20.4.1",
    "@eslint/js": "^10.0.1",
    "commitlint": "^20.4.1",
    "eslint": "^10.0.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.8.1"
  }
